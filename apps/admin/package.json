{
	"name": "@business-management/admin",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"dev": "vite --port=3001",
		"dev:mock": "vite --mode mock",
		"serve": "vite preview",
		"start": "vite",
		"test": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:ui": "vitest --ui --coverage.enabled=true",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@tailwindcss/vite": "catalog:",
		"@tanstack/react-router": "catalog:",
		"@tanstack/router-devtools": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"tailwindcss": "catalog:"
	},
	"devDependencies": {
		"@business-management/tailwind": "workspace:*",
		"@business-management/typescript": "workspace:*",
		"@faker-js/faker": "catalog:",
		"@tanstack/router-plugin": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"@vitejs/plugin-react-swc": "catalog:",
		"@vitest/coverage-v8": "catalog:",
		"@vitest/ui": "catalog:",
		"msw": "catalog:",
		"vite": "catalog:",
		"vite-tsconfig-paths": "catalog:",
		"vitest": "catalog:"
	},
	"packageManager": "pnpm@10.4.0",
	"engines": {
		"node": ">=22"
	}
}
